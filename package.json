{
  "name": "file-upload",
  "version": "0.1.6",
  "description": "file upload for digital.gov",
  "main": "index.node",
  "scripts": {
    "test": "cargo test",
    "cargo-build": "cargo build --message-format=json > cargo.log",
    "cross-build": "cross build --message-format=json > cross.log",
    "postcargo-build": "neon dist < cargo.log",
    "postcross-build": "neon dist -m /target < cross.log",
    "debug": "npm run cargo-build --",
    "build": "npm run cargo-build -- --release",
    "cross": "npm run cross-build -- --release",
    "install:binary": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@neon-rs/cli": "0.1.81"
  },
  "dependencies": {
    "cargo-cp-artifact": "^0.1.9"
  }
}